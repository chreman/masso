{
  "press":
    {
    "xml":{
      "availableLanguages":
        {"selector":"./{*}availableLanguages",
          "attribute":"text"},
      "category":
        {"selector":".//{*}categories/{*}category[@classification]",
          "attribute":"text"},
      "identifier":
        {"selector":"./{*}rcn",
          "attribute":"int"},
      "fulltext":
        {"selector":".//{*}article",
          "attribute":"text"},
      "keywords":
        {"selector":".//{*}categories/{*}category[@classification='sicCode']/{*}title",
          "attribute":"text"},
      "relatedProject":
        {"selector":".//{*}associations/{*}project[@type='relatedProject']/{*}rcn",
          "attribute":"int"},
      "title":
        {"selector":"./{*}title",
          "attribute":"text"},
      "publicationDate":
        {"selector":".//{*}contentCreationDate",
          "attribute":"text"},
      "lastUpdateDate":
        {"selector":".//{*}lastUpdateDate",
          "attribute":"text"},
      "referenceDocument":
        {"selector":".//{*}referenceDocument",
          "attribute":"text"}
      }
    },
  "calls":
    {
    "html":{
      "fulltext":
      {"selector":".//div[@class='tab-content']//p",
        "attribute":"text"},
      "links":
      {"selector":".//div[@class='tab-content']//p//a",
        "attribute":"href"},
      "title":
      {"selector":".//div[@class='well']/h3",
        "attribute":"text"},
      "publicationDate":
      {"selector":".//div[@class='well']/table[1]/tbody/tr[2]/td[2]",
        "attribute":"text"},
      "identifier":
      {"selector":".//div[@class='well']/table[1]/tbody/tr[1]/td[2]",
        "attribute":"text"},
      "typesOfAction":
      {"selector":".//div[@class='well']/table[2]/tbody/tr[1]/td[2]",
        "attribute":"text"}
    }
  }
}
